echo .
echo .
echo .
echo .
echo .
echo .
echo .
echo .
echo .
echo .
echo  ===============================================================
echo =                                                               =
echo =    Download / Install:    Git for Windows  (64-bit)           =
echo =                                                               =
echo =                       please wait...                          =
echo =                                                               =
echo  ===============================================================
