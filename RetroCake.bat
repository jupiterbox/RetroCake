%rkdir%\Tools\Wget\wget.exe -q https://github.com/git-for-windows/git/releases/download/v2.22.0.windows.1/PortableGit-2.22.0-64-bit.7z.exe -O "%rkdir%\Temp\git.zip"
mkdir %rkdir%\Tools\git
